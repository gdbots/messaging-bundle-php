{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jquery": true,
    "es6": true
  },
  "rules": {
    "no-unused-vars": 0,
    "object-shorthand": 0,
    "no-plusplus": 0,
    "no-param-reassign": 0,
    "global-require": 0,
    "import/no-dynamic-require": 0
  },
  "extends": "airbnb"
}
