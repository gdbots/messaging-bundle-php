{
  "name": "gdbots/pbjx-bundle",
  "homepage": "https://github.com/gdbots/pbjx-bundle-php",
  "description": "Symfony bundle that integrates gdbots/pbjx messaging tools.",
  "type": "symfony-bundle",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "gdbots/pbjx": "^2.2",
    "symfony/console": "^4.0",
    "symfony/framework-bundle": "^4.0",
    "twig/twig": "^2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "aws/aws-sdk-php-symfony": "^1.0",
    "ruflin/elastica": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Bundle\\PbjxBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Bundle\\PbjxBundle\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
