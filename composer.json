{
  "name": "gdbots/pbjx-bundle",
  "homepage": "https://github.com/gdbots/pbjx-bundle-php",
  "description": "Symfony bundle that integrates gdbots/pbjx messaging tools.",
  "type": "symfony-bundle",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.4",
    "gdbots/pbjx": "3.x-dev",
    "gdbots/query-parser": "2.x-dev",
    "gdbots/uri-template": "^1.1",
    "symfony/console": "^5.1",
    "symfony/framework-bundle": "^5.1",
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1",
    "aws/aws-sdk-php-symfony": "^2.2",
    "gdbots/acme-schemas": "^2.0",
    "ruflin/elastica": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Bundle\\PbjxBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Bundle\\PbjxBundle\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
