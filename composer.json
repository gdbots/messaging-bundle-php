{
  "name": "gdbots/pbjx-bundle",
  "homepage": "https://github.com/gdbots/pbjx-bundle-php",
  "description": "Symfony3 bundle that integrates gdbots/pbjx messaging tools.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "gdbots/pbj": "~1.1",
    "gdbots/pbjx": "dev-es as 1.0.0",
    "gdbots/schemas": "dev-es as 1.0.0",
    "symfony/symfony": "~3.2"
  },
  "require-dev": {
    "composer/composer": "1.0.*@dev",
    "phpunit/phpunit": "~4.3",
    "aws/aws-sdk-php-symfony": "~1.0",
    "ruflin/elastica": "~3.2"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Bundle\\PbjxBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Bundle\\PbjxBundle\\": "tests"
    }
  }
}
